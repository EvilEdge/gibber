{
  "name": "gibber",
  "version": "2.0.1",
  "description": "Audiovisual creative coding environment",
  "main": "js/index.js",
  "dependencies": {
    "gl-mat4": "^1.2.0",
    "gl-vec3": "^1.1.3",
    "node-static": "^0.7.11",
    "ws": "^7.3.1",
    "y-websocket": "1.3.12",
    "yjs": "^13.3.2"
  },
  "devDependencies": {
    "acorn": "^6.2.0",
    "acorn-walk": "^6.2.0",
    "browserify": "^16.2.0",
    "codemirror": "^5.55.0",
    "dotenv": "^10.0.0",
    "envify": "^4.1.0",
    "esmify": "^2.1.1",
    "gibber.audio.lib": "latest",
    "gibber.core.lib": "latest",
    "gibber.graphics.lib": "^2.0.4",
    "gulp": "^4.0.2",
    "gulp-replace": "^1.1.3",
    "gulp-uglify": "^3.0.2",
    "tern": "^0.24.1",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.1",
    "y-codemirror": "^2.0.9"
  },
  "scripts": {
    "start": "cd playground; node server.js",
    "clean": "cd node_modules && rm -r marching && rm -r gibber.audio.lib && rm -r gibber.graphics.lib && rm -r gibber.core.lib && rm -r gibberish-dsp && rm -r genish.js",
    "link": "npm link genish.js gibberish-dsp gibber.core.lib gibber.audio.lib marching gibber.graphics.lib",
    "build": "npx browserify playground/environment.js -p esmify > playground/bundle.js",
    "postinstall": "cd node_modules/marching; npm i; cd ../gibberish-dsp; npm link genish.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/charlieroberts/gibber.git"
  },
  "keywords": [
    "gibber",
    "audio",
    "music",
    "graphics",
    "raymarching",
    "shaders",
    "sequencing"
  ],
  "author": "Charlie Roberts <charlie@charlie-roberts.com> (http://www.charlie-roberts.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/charlieroberts/gibber/issues"
  }
}
