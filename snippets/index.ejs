  <% if( loadFile !== null) { %>
  <script type="text/javascript">
    window.loadFile = <%- loadFile %>
    window.isInstrument = <%- isInstrument %>
    //if( window.isInstrument ) alert("INSTRUMENT")
  </script>
  <% } %>
  
  <script src="share.uncompressed.js"></script>
  <script src="share-codemirror.js"></script>
  
